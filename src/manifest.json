{
    "manifest_version": 3,
    "name": "Wiremock extension",
    "description": "Chrome extension for managing stubs within your Wiremock instance. For more details on Wiremock visit http://wiremock.org/",
    "homepage_url": "https://github.com/mwinteringham/wiremock-chrome-extension",
    "version": "1.0.1",
    "action": {
        "default_title": "Let's get stubbing!",
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "http://localhost:8080/*",
        "http://127.0.0.1:8080/*",
        "http://localhost/*",
        "http://127.0.0.1/*"
    ],
    "icons": {
        "16": "assets/wiremock-concept-icon-01.png",
        "48": "assets/wiremock-concept-icon-01.png",
        "128": "assets/wiremock-concept-icon-01.png"
    }
}
